{
  "RabbitMQ": {
    "HostName": "localhost",
    "UserName": "admin",
    "Password": "admin123!",
    "VirtualHost": "/",
    "StreamName": "validations",
    "MaxRetries": 3,
    "RetryInterval": 1000
  },
  "Oracle": {
    "ConnectionString": "User Id=billettique;Password=billettique123!;Data Source=localhost:1521/BILLETDB;",
    "MinPoolSize": 5,
    "MaxPoolSize": 100,
    "ConnectionTimeout": 30,
    "CommandTimeout": 30
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000",
        "Protocols": "Http2"
      },
      "Https": {
        "Url": "https://localhost:5001",
        "Protocols": "Http2",
        "Certificate": {
          "Path": "certs/server.pfx",
          "Password": "cert123!"
        }
      }
    }
  },
  "Metrics": {
    "Enable": true,
    "Port": 9090,
    "Path": "/metrics",
    "EnableHealthCheck": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc": "Debug"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*"
}